<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<!-- Default appender -->
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-5level %logger{55}: %msg %n</pattern>
		</encoder>
	</appender>

	<root level="DEBUG">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
